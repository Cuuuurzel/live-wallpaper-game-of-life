package com.cuuuurzel.gollivewallpaper;

public class DummyWidget {

	float x, y, w, h;
	
	public DummyWidget( float x, float y, float w, float h ) {
		this.x = x;
		this.y = y;
		this.w = w;
		this.h = h;
	}
	
	public boolean collide( float px, float py ) {
		if ( x < px && px < x+w ) {
			if ( y < py && py < y+h ) {
				return true;
			}
		}		
		return false;
	}
}
