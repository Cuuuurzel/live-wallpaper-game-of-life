package com.cuuuurzel.gollivewallpaper;

public class DummyWidget {

	int x, y, w, h;
	
	public DummyWidget( int x, int y, int w, int h ) {
		this.x = x;
		this.y = y;
		this.w = w;
		this.h = h;
	}
	
	public boolean collide( int px, int py ) {
		if ( x < px && px < x+w ) {
			if ( y < py && py < y+h ) {
				return true;
			}
		}		
		return false;
	}
}
